{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"endOfLine": "lf",
	"jsxSingleQuote": false,
	"printWidth": 80,
	"proseWrap": "preserve",
	"semi": true,
	"singleQuote": true,
	"tabWidth": 3,
	"trailingComma": "all",
	"useTabs": false,
	"overrides": [
		{
			"files": [
				"**/*.astro"
			],
			"options": {
				"parser": "astro"
			}
		}
	]
}